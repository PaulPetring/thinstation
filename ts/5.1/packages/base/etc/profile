#/etc/profile: system-wide .profile file for ash.

trap "" INT QUIT TSTP

umask 022

if [ "$TERM" == "xinit" ] ; then
	. /etc/thinstation.global
	hide_cursor
	check_last_session
	cd /var/run
	exec /bin/xinit /etc/xinitrc -- /etc/xserverrc &> /var/run/applications/session.$$
fi

. /etc/thinstation.global --nofunctions
trap - INT QUIT TSTP

cd ~
alias ll='ls -l'
alias l='ls -l'
if [ -e /bin/e3vi ]; then
	alias vi='e3vi'
fi

