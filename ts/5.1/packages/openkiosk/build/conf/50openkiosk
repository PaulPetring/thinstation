# SESSION_#_OPENKIOSK_ADMIN_PASS="aa4e30e844a715a2989cef8ef57c038c"
#	set new administration password
#	This string is generated by running:
#	echo '<?php echo md5(base64_encode("newpass")); ?>' | php
#
# SESSION_#_OPENKIOSK_TOOLBAR_ON="false"
#	show/hide the toolbar	(default is true)
#
# SESSION_#_OPENKIOSK_TOOLBAR_BUTTONS="unified-back-forward-button,reload-button,stop-button,home-button,urlbar-container,print-button,zoom-control,reset-container,navigator-throbber"
#	set the toolbar buttons
#
# SESSION_#_OPENKIOSK_RESET_TIMER="5"
#	define the reset timer	(default is 1 minute)
#
# SESSION_#_OPENKIOSK_PROFILE="newprofile"
#	create a new profile
#
# SESSION_#_OPENKIOSK_WHITELIST_URL="ftp://yourserver/whilelist.txt"
#	URL location of config file, with whitelist URLs allowed to be viewed in
#       the kiosk browser

